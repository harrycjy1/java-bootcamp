<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<GridPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="300" prefWidth="300" hgap="10" vgap="10">
	<padding>
		<Insets topRightBottomLeft = "10"/>
	</padding>
	
	<children>
		
	
		<Button text = "${'Num\nLock'}"
			GridPane.rowIndex = "0" GridPane.columnIndex = "0" prefWidth="60" prefHeight="60"
			alignment="CENTER"/>
		<Button text = "/" GridPane.rowIndex = "0" GridPane.columnIndex = "1" 
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "*" GridPane.rowIndex = "0" GridPane.columnIndex = "2" 
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "-" GridPane.rowIndex = "0" GridPane.columnIndex = "3"
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "7" GridPane.rowIndex = "1" GridPane.columnIndex = "0"
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "4" GridPane.rowIndex = "2" GridPane.columnIndex = "0"
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "1" GridPane.rowIndex = "3" GridPane.columnIndex = "0"
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "8" GridPane.rowIndex = "1" GridPane.columnIndex = "1"
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "5" GridPane.rowIndex = "2" GridPane.columnIndex = "1"
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "2" GridPane.rowIndex = "3" GridPane.columnIndex = "1"
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "9" GridPane.rowIndex = "1" GridPane.columnIndex = "2"
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "6" GridPane.rowIndex = "2" GridPane.columnIndex = "2"
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "3" GridPane.rowIndex = "3" GridPane.columnIndex = "2"
		prefWidth="60" prefHeight="60" alignment="CENTER"/>
		<Button text = "+" GridPane.rowIndex = "1" GridPane.columnIndex = "3"
		GridPane.rowSpan="2" prefWidth="60" prefHeight="130" alignment="CENTER"/>
		<Button text = "Enter" GridPane.rowIndex = "3" GridPane.columnIndex = "3"
		GridPane.rowSpan="2" prefWidth="60" prefHeight="130" alignment="CENTER"/>
		<Button text = "0" GridPane.rowIndex = "4" GridPane.columnIndex = "0"
		GridPane.columnSpan="2" prefWidth="130" prefHeight="60" alignment="CENTER"/>
		<Button text = "." GridPane.rowIndex = "4" GridPane.columnIndex = "2"
		GridPane.columnSpan="2" prefWidth="60" prefHeight="60" alignment="CENTER"/>
		
		
		
		
		
	</children>
	
	
</GridPane>

